#define IDR_MENU1 10000
#define IDM_START 10001
#define IDM_QUIT 10002
#define IDM_SAVE 10003
#define IDR_VERSION1 1

#include <resource.h>

IDR_MENU1 MENU
BEGIN
  MENUITEM "开始",IDM_START
  MENUITEM "保存",IDM_SAVE
  MENUITEM "退出",IDM_QUIT
END

IDR_VERSION1 VERSIONINFO
FILEVERSION 1,0,0,0
PRODUCTVERSION 1,0,0,0
FILEOS 0x00000004
FILETYPE 0x00000001
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "080404B0"
    BEGIN
      VALUE "FileVersion", "1.0.0.0\0"
      VALUE "ProductVersion", "1.0.0.0\0"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0804, 0x04B0
  END
END

